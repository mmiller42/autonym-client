{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "eslint-config-airbnb",
  "rules": {
    "comma-dangle": [
      "error",
      "never"
    ],
    "indent": [
      "error",
      "tab"
    ],
    "max-len": [
      "error",
      {
        "code": 160,
        "tabWidth": 2,
        "ignoreComments": false,
        "ignoreTrailingComments": false
      }
    ],
    "no-else-return": "off",
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "no-tabs": "off",
    "no-use-before-define": [
      "error",
      "nofunc"
    ]
  }
}
